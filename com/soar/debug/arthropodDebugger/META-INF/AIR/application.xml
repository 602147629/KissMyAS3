<?xml version="1.0" encoding="UTF-8" standalone="no" ?><application xmlns="http://ns.adobe.com/air/application/1.0">

  <id>com.carlcalderon.Arthropod</id>

  <version>1.00</version>

  <filename>Arthropod</filename>

  <description>The easy Flash / AIR debugger</description>

  <name>Arthropod</name>

  <copyright>Carl Calderon 2008</copyright>

  <initialWindow>
    <content>Arthropod.swf</content>
    <systemChrome>none</systemChrome>
    <transparent>true</transparent>
    <visible>true</visible>
    <maximizable>false</maximizable>
    <minimizable>true</minimizable>
    <resizable>true</resizable>
  </initialWindow>

  <icon>
    <image48x48>icons/48_ico.png</image48x48>
    <image128x128>icons/128_ico.png</image128x128>
    <image32x32>icons/32_ico.png</image32x32>
    <image16x16>icons/16_ico.png</image16x16>
  </icon>

  <customUpdateUI>false</customUpdateUI>

  <allowBrowserInvocation>false</allowBrowserInvocation>


</application>
